<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CE_Enabled_Effective_Date_Co_Mandatory</fullName>
    <active>true</active>
    <description>CE Enabled Effective Date Co Mandatory</description>
    <errorConditionFormula>AND(!ISBLANK(TEXT( FS_CE_Enabled_New__c )),
(ISBLANK(FS_CE_Enabled_Effective_Date__c )),NOT(CONTAINS($Profile.Name, $Label.Exclude_User)))</errorConditionFormula>
    <errorDisplayField>FS_CE_Enabled_Effective_Date__c</errorDisplayField>
    <errorMessage>This field is mandatory if enable consumer engagement is selected</errorMessage>
</ValidationRule>
