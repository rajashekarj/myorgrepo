<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>X7000_Brand_Select_Mandatory_if_Eff_dt</fullName>
    <active>true</active>
    <errorConditionFormula>And(ISBlank(Text(FS_7000_Series_Brands_Selection_New__c)),Not(isBlank(FS_Brand_Selection_Value_Effective_Date__c)), NOT(CONTAINS($Profile.Name, $Label.Exclude_User)))</errorConditionFormula>
    <errorDisplayField>FS_7000_Series_Brands_Selection_New__c</errorDisplayField>
    <errorMessage>7000 Series Brands Option Selections. is mandatory if we have &#39;Brand Selections Effective Date&#39;</errorMessage>
</ValidationRule>
