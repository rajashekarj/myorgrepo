<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Outlet_Field_Locked</fullName>
    <active>false</active>
    <description>To lock Outlet Field</description>
    <errorConditionFormula>NOT(ISNEW())&amp;&amp; ISCHANGED(FS_Outlet__c) &amp;&amp; NOT     ISPICKVAL(FS_Status__c, &#39;New&#39;) &amp;&amp; FS_Outlet__r.Is_Default_FET_International_Outlet__c = false</errorConditionFormula>
    <errorDisplayField>FS_Outlet__c</errorDisplayField>
    <errorMessage>Outlet linked to an Outlet Dispenser can&#39;t be changed</errorMessage>
</ValidationRule>
