<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FS_SP_Scheduled_Alert_Date__c</fullName>
    <externalId>false</externalId>
    <formula>CASE( 
MOD( (FS_SP_Scheduled_Date__c ) - DATE(1900, 1, 7), 7), 
0,(FS_SP_Scheduled_Date__c  - 5), 
5,(FS_SP_Scheduled_Date__c - 4),	
6,(FS_SP_Scheduled_Date__c  - 4), 
(FS_SP_Scheduled_Date__c - 6) 
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>SP Scheduled Alert Date Alert</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Date</type>
</CustomField>
