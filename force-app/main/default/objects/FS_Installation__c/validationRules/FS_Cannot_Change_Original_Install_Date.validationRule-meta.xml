<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FS_Cannot_Change_Original_Install_Date</fullName>
    <active>true</active>
    <errorConditionFormula>IF(!ISBLANK(PRIORVALUE(FS_Original_Install_Date__c)) &amp;&amp; NOT(ISBLANK(FS_Original_Install_Date__c)) &amp;&amp; PRIORVALUE(FS_Original_Install_Date__c)!=FS_Original_Install_Date__c,true,
IF(!ISBLANK(PRIORVALUE(FS_Original_Install_Date__c)) &amp;&amp; ISBLANK(FS_Original_Install_Date__c),true,false))&amp;&amp; NOT(CONTAINS($Profile.Name, $Label.Exclude_User))</errorConditionFormula>
    <errorDisplayField>FS_Original_Install_Date__c</errorDisplayField>
    <errorMessage>Please use rescheduled section to change the date</errorMessage>
</ValidationRule>
