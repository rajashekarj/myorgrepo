<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FS_Cannot_Use_Date2_before_Date1</fullName>
    <active>false</active>
    <errorConditionFormula>IF( AND(ISCHANGED(FS_ReschedInstallDate2__c), ISBLANK(FS_ReschedInstallDate1__c)), true, false )</errorConditionFormula>
    <errorMessage>You cannot use Rescheduled Install Date 2 before Rescheduled Install Date 1.</errorMessage>
</ValidationRule>
