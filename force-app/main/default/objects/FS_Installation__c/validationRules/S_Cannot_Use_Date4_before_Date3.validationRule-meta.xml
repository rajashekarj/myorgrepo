<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>S_Cannot_Use_Date4_before_Date3</fullName>
    <active>false</active>
    <errorConditionFormula>IF( AND(ISCHANGED(FS_ReschedInstallDate4__c), ISBLANK(FS_ReschedInstallDate3__c)), true, false )</errorConditionFormula>
    <errorMessage>You cannot use Rescheduled Install Date 4 before Rescheduled Install Date 3.</errorMessage>
</ValidationRule>
