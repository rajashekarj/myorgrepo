<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FS_Cannot_Use_Date5_before_Date4</fullName>
    <active>false</active>
    <errorConditionFormula>IF( AND(ISCHANGED(FS_ReschedInstallDate5__c), ISBLANK(FS_ReschedInstallDate4__c)), true, false )</errorConditionFormula>
    <errorMessage>You cannot use Rescheduled Install Date 5 before Rescheduled Install Date 4.</errorMessage>
</ValidationRule>
