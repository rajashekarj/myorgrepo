<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FOT_Check_RuleSetName_For_Spaces</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX(Name ,&quot;([a-zA-Z0-9-_])*&quot;))</errorConditionFormula>
    <errorMessage>Please enter Ruleset name without spaces and special characters (like &amp;!#%etc..).</errorMessage>
</ValidationRule>
