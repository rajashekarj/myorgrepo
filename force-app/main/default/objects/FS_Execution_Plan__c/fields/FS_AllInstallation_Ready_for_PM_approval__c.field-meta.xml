<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FS_AllInstallation_Ready_for_PM_approval__c</fullName>
    <description>it will become TRUE when all installations sent for PM Approval</description>
    <externalId>false</externalId>
    <formula>IF( AND(FS_of_Installations__c&gt;0,(FS_Installations_ready_for_PM_approval__c + FS_of_Installations_Cancelled__c)  = FS_of_Installations__c) , true, false)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>All Installations Ready for PM approval</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
