<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Global_Center_Case</fullName>
    <booleanFilter>1 AND 2 AND 3 AND 4 AND 5</booleanFilter>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>FS_Platform_Type__c</columns>
    <columns>Issue_Type__c</columns>
    <columns>CASES.STATUS</columns>
    <columns>CASES.SUBJECT</columns>
    <columns>NAME</columns>
    <columns>CREATEDBY_USER.ALIAS</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <columns>CASES.RECORDTYPE</columns>
    <columns>CASES.CREATED_DATE</columns>
    <columns>Case_Age_Days__c</columns>
    <columns>Case_Category__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.STATUS</field>
        <operation>equals</operation>
        <value>On Hold,Escalated,New,Assigned,Close,Closed,Complete,Validation</value>
    </filters>
    <filters>
        <field>Issue_Type__c</field>
        <operation>equals</operation>
        <value>HIO,STA Request,FSGC,Connectivity,Agent Proactive,Questions/Inquires,FA Testing</value>
    </filters>
    <filters>
        <field>CASES.CREATED_DATE</field>
        <operation>greaterThan</operation>
        <value>1/1/2016 12:00 AM</value>
    </filters>
    <filters>
        <field>Escalate_to_Global_Center__c</field>
        <operation>equals</operation>
        <value>1</value>
    </filters>
    <filters>
        <field>Sub_Status__c</field>
        <operation>notEqual</operation>
        <value>Closed,Closed/ Other,GC Completed</value>
    </filters>
    <label>Global Center - Escalated</label>
    <sharedTo>
        <group>Freestyle_Support_Agents</group>
    </sharedTo>
</ListView>
