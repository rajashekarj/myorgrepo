<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>FS_Local_Time__c</fullName>
    <externalId>false</externalId>
    <formula>IF(OR(MONTH(DATEVALUE(LastModifiedDate))==1,
      MONTH(DATEVALUE(LastModifiedDate))==2,
      MONTH(DATEVALUE(LastModifiedDate))==3,
      MONTH(DATEVALUE(LastModifiedDate))==11,
      MONTH(DATEVALUE(LastModifiedDate))==12) , 
	  LEFT(Text(LastModifiedDate-0.2083),LEN(Text(LastModifiedDate-0.2083))-1),
	  LEFT(Text(LastModifiedDate-0.1666),LEN(Text(LastModifiedDate-0.1666))-1))</formula>
    <label>Local Time</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
