<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Enable_CE_Effective_Date_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(!ISBLANK(TEXT(FS_Enable_Consumer_Engagement__c)), 
(ISBLANK(FS_Enable_CE_Effective_Date__c)) 
)</errorConditionFormula>
    <errorDisplayField>FS_Enable_CE_Effective_Date__c</errorDisplayField>
    <errorMessage>This field is mandatory if &#39;FET Enable Consumer Engagement&#39; is selected.</errorMessage>
</ValidationRule>
