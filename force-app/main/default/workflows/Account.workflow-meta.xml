    <rules>
        <fullName>Approval Cartridge Ordering Method for Bottler</fullName>
        <actions>
            <name>Approval_for_Bottler_Ordered_and_Delivery_Needed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <formula>AND(RecordType.Name = &quot;FS Outlet&quot;,OR($UserRole.Name = &#39;NDO&#39;, $Profile.Name = &#39;FS NDO_P&#39;,$User.FS_Permission_Sets__c =&#39;Bottler_Approval_Access&#39;), ISCHANGED(FS_VMS_Customer__c),text(FS_VMS_Customer__c)=&#39;Bottler&#39;,!OR(ISCHANGED(FS_Requested_Order_Method__c),ISCHANGED(FS_Requested_Delivery_Method__c)))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Approval for Bottler Ordered and Delivery</fullName>
        <actions>
            <name>Approval_for_Bottler_Ordered_and_Delivery_Needed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Approval for Bottler Ordered andDelivery</description>
        <formula>AND(RecordType.Name = &quot;FS Outlet&quot;,OR($UserRole.Name = &#39;NDO&#39;, $Profile.Name = &#39;FS NDO_P&#39;,$User.FS_Permission_Sets__c =&#39;Bottler_Approval_Access&#39;,$Profile.Name = &#39;FS FPS_P&#39;,$Profile.Name = &#39;FS COM_P&#39;,$Profile.Name = &#39;FS Sales_P&#39;,$Profile.Name = &#39;FS CPO_P&#39;,$Profile.Name = &#39;FS AC_P&#39;,$Profile.Name = &#39;FS PM_P&#39;,$Profile.Name = &#39;FS Admin PM_P&#39;), AND((OR( ISPICKVAL(FS_Requested_Delivery_Method__c,&#39;Bottler&#39;), ISPICKVAL(FS_Requested_Delivery_Method__c,&#39;Direct Ship&#39;))), ISPICKVAL(FS_Requested_Order_Method__c,&#39;Bottler&#39;)), PRIORVALUE(FS_Approved_Bottler_Order_Delivery__c)&lt;&gt;true , OR(isChanged(FS_Requested_Delivery_Method__c), isChanged(FS_Requested_Order_Method__c)),  FS_Approved_Bottler_Order_Delivery__c =false)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Approval for Distributor Ordered and Delivery</fullName>
        <actions>
            <name>Approval_for_Distributor_Ordered_and_Delivery_Needed</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Approval for Distributor Ordered and Delivery</description>
        <formula>AND(  RecordType.Name = &quot;FS Outlet&quot;,$UserRole.Name&lt;&gt;&#39;NDO&#39;, OR(isChanged(FS_Requested_Delivery_Method__c), isChanged(FS_Requested_Order_Method__c)),  OR(  AND(ISPICKVAL(FS_Requested_Delivery_Method__c,&#39;Distributor&#39;),  NOT(OR(ISBLANK(FS_Final_Order_Text_value__c),ISBLANK(FS_Final_Delivery_text_value__c)))),   AND(OR(FS_Final_Order_Text_value__c=&#39;Distributor&#39;,FS_Final_Order_Text_value__c=&#39;VMS&#39;,FS_Final_Order_Text_value__c=&#39;CokeSmart&#39;),FS_Final_Delivery_text_value__c=&#39;Distributor&#39;,  NOT(AND(OR(ISPICKVAL(FS_Requested_Order_Method__c,&#39;Distributor&#39;),ISPICKVAL(FS_Requested_Order_Method__c,&#39;VMS&#39;),ISPICKVAL(FS_Requested_Order_Method__c,&#39;CokeSmart&#39;) ),ISPICKVAL(FS_Requested_Delivery_Method__c,&#39;Distributor&#39;)))  )  ))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Auto SAP</fullName>
        <actions>
            <name>KO_Record_Type</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>Update KO record type for International Outlet...</description>
        <formula>AND(RecordType.Name = &quot;BoE Outlet&quot;,Autogenerated_SAP__c  &lt;&gt; &#39;&#39; ,   ISNEW() )</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Bottler Account name capture on BoE Outlet</fullName>
        <actions>
            <name>Bottler_field_capture</name>
