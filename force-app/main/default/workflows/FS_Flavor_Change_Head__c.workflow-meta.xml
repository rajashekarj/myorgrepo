        </actions>
        <active>true</active>
        <description>NDO Notification for Direct Ship</description>
        <formula>AND(OR(RecordType.Name==&#39;FS Flavor Change Outlet&#39;,RecordType.Name==&#39;FS Flavor Change Installation&#39;) , NOT(OR( ISPICKVAL(Is_customer_switching_back_to_DirectShip__c, &#39;No&#39;),ISPICKVAL(Is_customer_switching_back_to_DirectShip__c, &#39;None&#39;)) ),  TEXT(FS_FC_Type__c)==&#39;Workflow Flavor Change&#39; , ISPICKVAL(FS_NDO_Approval__c ,&#39;Rejected&#39;),TEXT(PRIORVALUE(Is_customer_switching_back_to_DirectShip__c)) &lt;&gt; TEXT(Is_customer_switching_back_to_DirectShip__c) )</formula>
