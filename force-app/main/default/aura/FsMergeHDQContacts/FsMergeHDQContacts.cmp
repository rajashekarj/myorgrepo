<aura:component implements="flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickAction,force:appHostable,lightning:actionOverride" 
                access="global" controller="FSOutletContactsRelatedController">
	<aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:attribute name="phoneNumber" type="string" />
    <aura:attribute name="hqId" type="Id" />
    <div class="slds-clearfix">
        <div class="slds-float_right">
            <lightning:button variant="neutral" label="Merge HDQ Contacts" onclick="{! c.handleClick }" />
            <lightning:button variant="neutral" label="New Contact" onclick="{! c.handleClickContact }" />         
        </div>
    </div>
</aura:component>