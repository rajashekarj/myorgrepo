                                                      } else {                                    
                                                          document.getElementById("errorPopup").innerHTML = "<div class=\"conErrMsg\"><b style=\"color:#c00;\">Error: </b><br/>" + event.message + "</pre></div>";
                                                      }
                                                  }, 
                                                  {escape: true}
                                                 );
        
    }
    
    
