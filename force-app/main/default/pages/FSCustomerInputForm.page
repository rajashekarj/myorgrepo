    function saveContact(){                 
        document.getElementById("errorPopup").innerHTML = "";
        document.getElementById("errorLastName").innerHTML = "";
        document.getElementById("errorTitle").innerHTML = "";
        document.getElementById("errorPhone").innerHTML = "";
        document.getElementById("errorEmail").innerHTML = "";               
        var contactRecord = new Contact();          
        contactRecord.FirstName = $('[id$="cntFirstName"]').val();
        
        if($('[id$="LastName"]').val() != '' && $('[id$="LastName"]').val() != null ){                          
            contactRecord.LastName = $('[id$="cntLastName"]').val();                            
        }else{
            document.getElementById("errorPopup").innerHTML = "<div class=\"conErrMsg\"><b style=\"color:#c00;\">Error: </b><br/>Required field missing: Last Name </pre></div>";
            document.getElementById("errorLastName").innerHTML = "<b>Error: </b>You must enter a value";
            return null;
        }
        if($('[id$="cntTitle"]').val() != '' && $('[id$="cntTitle"]').val() != null ){                          
            contactRecord.FS_Title__c = $('[id$="cntTitle"]').val();
        }else{
            document.getElementById("errorPopup").innerHTML = "<div class=\"conErrMsg\"><b style=\"color:#c00;\">Error: </b><br/>Required field missing: Title </pre></div>";           
            document.getElementById("errorTitle").innerHTML = "<b>Error: </b>You must enter a value";
            return null;
        }
        if($('[id$="cntPhone"]').val() != '' && $('[id$="cntPhone"]').val() != null ){                          
            contactRecord.phone = $('[id$="cntPhone"]').val();
        }else{
            document.getElementById("errorPopup").innerHTML = "<div class=\"conErrMsg\"><b style=\"color:#c00;\">Error: </b><br/>Required field missing: Phone </pre></div>";
            document.getElementById("errorPhone").innerHTML = "<b>Error: </b>You must enter a value";
            return null;
        }
        if($('[id$="cntEmail"]').val() != '' && $('[id$="cntEmail"]').val() != null ){                          
            contactRecord.email = $('[id$="cntEmail"]').val();
        }else{
            document.getElementById("errorPopup").innerHTML = "<div class=\"conErrMsg\"><b style=\"color:#c00;\">Error: </b><br/>Required field missing: Email </pre></div>";
            document.getElementById("errorEmail").innerHTML = "<b>Error: </b>You must enter a value";
            return null;
        }
        if($('[id$="cntRole"]').val() != '' && $('[id$="cntRole"]').val() != null ){
            contactRecord.FS_Execution_Plan_Role__c = $('[id$="cntRole"]').val();
        }
        
        Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.FSCustomerInputFormController.createContact}',contactRecord,                  
                                                  function(result, event){
                                                      if (event.status) {                    
