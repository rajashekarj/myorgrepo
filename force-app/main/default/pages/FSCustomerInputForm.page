            "FS_CIF_SiteAssessor__mdt" custom Metadata recrods"*/
            function changeVar(abc){
                var fieldid;
                if(abc.includes("Platform_for_Install")){
                	fieldid = abc.split("Platform_for_Install");
                }
                if(abc.includes("SAType")){
                    fieldid = abc.split("SAType");
                }
                var siteAsseValid = fieldid[0]+'Platform_for_Install';
                var SATypeid = fieldid[0]+'SAType';
                var Siteassid = fieldid[0]+'siteAssessor';
                var siteAsseVal=document.getElementById(siteAsseValid).value;
                debugger;
                var result = sforce.connection.query("SELECT Label, Site_Assessor__c from FS_CIF_SiteAssessor__mdt");
                records = result.getArray("records");
                var idSA="[id$=siteAssessor]";
                if(document.getElementById(SATypeid).value != '' && 
                   document.getElementById(SATypeid).value != 'SA Not Required - OSM approval required for local market'){           
                    var finalval = '';
                    for(var i=0; i< records.length; i++){
                        var rec = records[i];
                        if(siteAsseVal == rec.Label){
                            finalval = rec.Site_Assessor__c; 
                        }
                    }
                    document.getElementById(Siteassid).value = finalval;
                }
            }
            
