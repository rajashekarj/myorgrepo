<apex:page standardController="FS_Association_Brandset__c" sidebar="false">
<apex:form >
<apex:pageblock >
<apex:pageblockSection title="Associate Brandset" columns="1" >
<apex:outputField value="{!FS_Association_Brandset__c.FS_Platform__c}" label="Platform"  />
<apex:inputField value="{!FS_Association_Brandset__c.FS_Brandset__c}" label="Brandset"  />
<apex:outputField value="{!FS_Association_Brandset__c.FS_Headquarters__c}"  label="Headquarter"  rendered="{!FS_Association_Brandset__c.FS_Headquarters__c!=null}" />
<apex:outputField value="{!FS_Association_Brandset__c.FS_Installation__c}"  label="Installation"  rendered="{!FS_Association_Brandset__c.FS_Installation__c!=null}"/>
<apex:outputField value="{!FS_Association_Brandset__c.FS_Outlet_Dispenser__c}"  label="Outlet Dispenser"  rendered="{!FS_Association_Brandset__c.FS_Outlet_Dispenser__c!=null}"/>
<apex:outputField value="{!FS_Association_Brandset__c.FS_Country__c}"/>
</apex:pageblockSection>
<apex:pageBlockButtons location="bottom" >
    <apex:commandButton value="Save"  action="{!save}"/>   
     <apex:commandButton value="Cancel"  action="{!Cancel}"/> 
</apex:pageBlockButtons>
        
</apex:pageblock>
</apex:form>

</apex:page>