<apex:page standardController="FS_Outlet_Dispenser__c" sidebar="false" showHeader="false">
  
  <apex:iframe id="iframeId" src="" scrolling="true" />   
  
  <script src="/soap/ajax/39.0/connection.js" type="text/javascript"></script>
    <script type="text/javascript">
       sforce.connection.sessionId='{!GETSESSIONID()}';
    </script>
   <script>
     var dispenserId='{!FS_Outlet_Dispenser__c.Id}';
   
     var query = sforce.connection.query("SELECT Id,FS_Outlet_Dispenser__c FROM FS_Association_Brandset__c "+
                                                 " WHERE FS_Outlet_Dispenser__c='"+ dispenserId+"'");
                                                 
     var records = query.getArray("records");
     
     if(records.length>0){
         var associationBrandsetId=records[0].Id;
         document.getElementById('iframeId').src='/apex/FSAssociationBrandsetHistory?Id='+associationBrandsetId;
     }
     
   </script>
   


</apex:page>